<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <ApplicationIcon>cutt_ly.ico</ApplicationIcon>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>Cutt.ly</PackageId>
    <Authors>Miguel Oliveira &amp; Paulo HS</Authors>
    <Company>MiguelSoft</Company>
    <Product>Cutt.ly</Product>
    <Description>Library for using cutt.ly APIs in CSharp</Description>
    <PackageProjectUrl>https://github.com/eimigueloliveir/Cutt.ly</PackageProjectUrl>
    <PackageIcon>cutt_ly.png</PackageIcon>
    <RepositoryUrl>https://github.com/eimigueloliveir/Cutt.ly</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="cutt_ly.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\Downloads\cutt_ly.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
